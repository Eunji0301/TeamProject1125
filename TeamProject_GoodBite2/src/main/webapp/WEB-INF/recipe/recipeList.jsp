<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="ko">
<head>
<title>레시피 커뮤니티</title>
<style>
body {
	font-family: 'Arial', sans-serif;
	margin: 0;
	padding-top: 20px;
	padding-bottom: 20px;
}

/* 공통 CSS */
.inner {
	position: static;
	width: auto;
	max-width: 1000px;
	padding: 0 10px;
	margin: 0 auto;
}

.Inner {
	width: 1180px;
	margin: 0 auto;
	position: relative;
}

@media ( max-width : 1299px) {
	.inner {
		padding-left: 16px;
		padding-right: 16px;
	}
}

/* 레시피 커뮤니티 */
.container {
	width: 800px;
	margin: 100px auto;
	text-align: center;
	padding: 20px;
	background-color: #ffffff;
	border: 3px solid #CDC8E7;
	border-radius: 8px;
	box-sizing: border-box;
}

.header {
	font-size: 24px;
	font-weight: bold;
	color: #8475BC;
	background-color: #CDC8E7;
	padding: 15px;
	border-radius: 8px;
	margin-bottom: 20px;
	border: 2px solid #CDC8E7;
	box-sizing: border-box;
}

.tags {
	display: grid;
	grid-template-columns: repeat(5, 1fr); /* 버튼 5개씩 한 줄에 배치 */
	gap: 10px;
	justify-content: center;
	margin-bottom: 20px;
}

.tags button {
	background-color: #CDC8E7;
	color: #fff;
	border: none;
	border-radius: 4px;
	padding: 10px 15px;
	font-size: 16px;
	cursor: pointer;
	color: #8475BC;
}

.tags button:hover {
	background-color: #8475BC;
	color: white;
}

.recipes {
	display: grid;
	grid-template-columns: repeat(3, 1fr); /* 세 개씩 배치 */
	gap: 20px;
}

.recipe {
	text-align: center;
}

.recipe img {
	width: 100%;
	height: auto;
	border-radius: 8px;
	border: 2px solid #CDC8E7;
}

.recipe p {
	font-size: 16px;
	font-weight: bold;
	margin-top: 10px;
	color: #8475BC
}

.pagination {
	margin-top: 20px;
	display: flex;
	justify-content: center;
	align-items: center;
	gap: 10px;
}

.pagination button, .pagination span {
	background-color: #CDC8E7;
	color: #fff;
	border: none;
	border-radius: 4px;
	padding: 5px 10px;
	cursor: pointer;
}

.pagination span {
	cursor: default;
}

.pagination button:hover {
	background-color: #8475BC;
}

.pagination .active {
	background-color: #8475BC;
}
</style>
</head>
<body>
	<!-- <header>헤더</header> -->
	<%@ include file="../common/headerBefore.jsp"%>
	<!-- 레시피 커뮤니티 -->
	<div class="container">
		<div class="header">레시피</div>
		<div class="tags">
			<button>#고기</button>
			<button>#순두부</button>
			<button>#버섯</button>
			<button>#아스파라거스</button>
			<button>#계란</button>
			<button>#굴</button>
			<button>#콩</button>
			<button>#사과</button>
			<button>#감자</button>
			<button>#부추</button>
			<button>#오이</button>
			<button>#참치</button>
			<button>#가지</button>
			<button>#토마토</button>
			<button>#곤약</button>
			<button>#시금치</button>
			<button>#브로콜리</button>
			<button>#아보카도</button>
			<button>#고구마</button>
			<button>#멜론</button>
			<button>#키위</button>
			<button>#호두</button>
		</div>
		<div class="recipes">
			<div class="recipe">
					<img
						src="data:image/jpeg;base64,"
						alt="라따뚜이">

				<p>라따뚜이</p>
			</div>
			<div class="recipe">
				<img
					src="data:image/jpeg;base64,"
					alt="김치찌개">
				<p>김치찌개</p>
			</div>
			<div class="recipe">
				<img
					src="data:image/jpeg;base64,"
					alt="비빔밥">
				<p>비빔밥</p>
			</div>
			<div class="recipe">
				<img
					src="data:image/jpeg;base64,"
					alt="갈비탕">
				<p>갈비탕</p>
			</div>
		</div>
		<div class="pagination">
			<button>&laquo;</button>
			<button class="active">1</button>
			<button>2</button>
			<button>3</button>
			<button>4</button>
			<button>5</button>
			<button>6</button>
			<button>7</button>
			<button>8</button>
			<button>9</button>
			<button>10</button>
			<button>&raquo;</button>
		</div>
	</div>
	<%@ include file="../common/footer.jsp"%>
</body>
</html>
